{
  "extends": "./tsconfig.base.json",
  "exclude": [
    "esm",
    "examples",
    "tests",
    "dist",
    ".tsbuildinfo",
    "vite.config.ts.timestamp-*.mjs",
    "packages",
    "node_modules",
    "*.code-workspace",
    ".vscode",
    "pnpm-lock.yaml"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo/others.tsbuildinfo",
    "rootDir": ".",
    "outDir": "dist/others",
    "declarationDir": "dist/others/dts"
  }
}